#!/usr/bin/env mkb
# Builder mkb file for the java portion of the Heyzap extension on android
platform JAVA

files
{
    (source/android)
    Heyzap.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"
librarypath "incoming/heyzap-ads-sdk.jar"

option output-name=lib/android/Heyzap.jar
